#include <stdio.h>
#include <math.h>

int main(){
int i;
printf("Inserire numero istogramma");
scanf("%d",&i);
for (int n=0;n!=i;n++){
printf("*");}
printf("\n");
}
for (;i!=0;){
printf("Inserire numero istogramma");
scanf("%d",&i);
for (int n=0;n!=i;n++){
printf("*");}
printf("\n");
}

return 0;		
	}
