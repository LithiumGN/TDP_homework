#include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main(){
int* q;
q=(int *)malloc(3*sizeof(int));
printf("Inserire numero");
scanf("%d",q+2);
q+1=0
while (*(q+2)!=0){
    if (*(q+1)==0){
        *q=*(q+2);
        }
    else {
        if (*(q+2)<*q) {
        *q=*(q+2);}
        if (*(q+2)==0){
            if (0>*q){
                *q=0;
                }
            break;
            }
        }
    *(q+1)=1;
    printf("Inserire numero");
    scanf("%d",q+2);
    }
    printf("\n");
    printf("%d",*q+2);
    
return 0;		
}

